<p id="notice"><%= notice %></p>

<div id="bottomMenu">
  <%= link_to '<div class="bottomMenu">Back</div>'.html_safe, root_path %>
  <%= link_to '<div class="bottomMenu">New User</div>'.html_safe, new_user_path %>


</div>


<table id="jobListing">
  <thead>
    <tr>
      
      <th>Name</th>
      <th>Phone</th>
    
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      
        <tr>
          
          <td><a href="/users/<%= user.id  %>"><%= user.name %></a></td>
          <td><a href="/users/<%= user.id  %>"><%= user.phone %></a></td>

        </tr>

      
    <% end %>
  </tbody>
</table>

<br>


