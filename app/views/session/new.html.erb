

<h2 class="loginH2"> Please enter your login details </h2>
<%= form_tag '/login' do %>

<div id="login">
  <p>
    <%= label_tag 'email' %>
    <%= text_field_tag 'email' %>
  </p>

   <p>
    <%= label_tag 'password' %>
    <%= password_field_tag 'password'%>
  </p>
</div>
  <p>
    <%= submit_tag 'Login' %>
  </p>
  


<% end %>
